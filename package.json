{
  "name": "sinon-har-server",
  "version": "0.3.0",
  "description": "Auto mocks your server from a recorded .har file. Don't let your mocks lie to you!",
  "main": "sinon-har-server.js",
  "scripts": {
    "lint": "semistandard",
    "test": "semistandard && nyc --nycrc-path=test/nyc.conf.js node_modules/mocha/bin/_mocha",
    "travis": "npm test && coveralls < output/coverage/lcov.info"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/larsthorup/sinon-har-server.git"
  },
  "keywords": [
    "sinon",
    "http",
    "fakeServer",
    "http",
    "archive",
    "har"
  ],
  "author": "Lars Thorup <lars@zealake.com> (http://github.com/larsthorup)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/larsthorup/sinon-har-server/issues"
  },
  "homepage": "https://github.com/larsthorup/sinon-har-server#readme",
  "files": [
    "sinon-har-server.js"
  ],
  "devDependencies": {
    "chai": "^4.2.0",
    "coveralls": "^3.0.9",
    "mocha": "^9.1.1",
    "nyc": "^15.0.0",
    "semistandard": "^16.0.1",
    "sinon": "^11.1.2"
  }
}
